  "version": "0.0.6",
  "description": "rescheme",
  "main": "index.js",
  "scripts": {
    "test": "nodeunit test/*",
   "coveralls": "jscoverage lib && GPXPARSE_COV=1 nodeunit --reporter=lcov test | coveralls",
